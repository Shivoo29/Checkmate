# PostgreSQL Configuration
POSTGRES_USER=checkmate_user
POSTGRES_PASSWORD=CHANGE_THIS_TO_SECURE_PASSWORD

# JWT Secret (generate with: openssl rand -hex 32)
JWT_SECRET_KEY=CHANGE_THIS_TO_RANDOM_STRING

